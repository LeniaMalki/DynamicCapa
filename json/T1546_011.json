{
    "Name": "Event Triggered Execution: Application Shimming",
    "Id": "T1546.011",
    "Tactics": ["Persistence", "Privilege Escalation"],
    "Platforms": ["Windows"],
    "Permission required" : ["Administrator"],
    "Registry" : {
        "Type" : ["BEHwA"],
        "Cat" : ["REGISTRY"],
        "Sym" : ["NtCreateKey", "NtSetValueKey"],
        "Arg" : ["\\\\Microsoft\\\\Windows NT\\\\CurrentVersion\\\\AppCompatFlags\\\\InstalledSDB$",
                "\\\\Microsoft\\\\Windows NT\\\\CurrentVersion\\\\AppCompatFlags\\\\Custom$"]
    }
}